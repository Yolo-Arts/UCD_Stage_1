#lang racket

(define qualify
  (lambda (E L)
    (cond ( (null? L) (list E))
          ( (equal? (car L) E) (append (cons 'really L) (qualify E (cdr L)) ) ) 
          ( #t (cons (car L) (qualify E (cdr L)) ))
          )))

