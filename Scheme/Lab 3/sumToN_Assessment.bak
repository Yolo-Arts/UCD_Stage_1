#lang racket

(define sumToN
  (lambda (N L)
    (cond ( #t (+ 2 3) )
          )))

; (sumToN 2 '(1 2) ) will return '(1 2)
; (sumToN 4 '(1 2 2 3)) will return (1 3) (2 2))

(define sum
  (lambda (N L Target)
    (cond ( (null? L) (if (equal? N Target) N '()) )
          ( #t (sum N (cdr L) (- Target N)) )
          )))